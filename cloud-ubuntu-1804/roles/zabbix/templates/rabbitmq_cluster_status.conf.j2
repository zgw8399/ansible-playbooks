# Status
UserParameter=rabbitmq.status.run_queue,sudo rabbitmqctl status | grep -o 'run_queue,[0-9]*' | cut -d ',' -f2 | tr -d \"
# Cluster status
UserParameter=rabbitmq.cluster.name,sudo rabbitmqctl cluster_status | grep 'cluster_name,*' | cut -d '"' -f2
UserParameter=rabbitmq.cluster.node_name,sudo rabbitmqctl cluster_status | grep 'Cluster status of node' | awk '{print $5}' | cut -d "'" -f2
UserParameter=rabbitmq.cluster.nodes_count,sudo rabbitmqctl cluster_status | sed -n 2,3p | cut -d '[' -f4 | grep -o ',' | wc -l
UserParameter=rabbitmq.cluster.nodes_running_count,sudo rabbitmqctl cluster_status | sed -n 4p | cut -d '[' -f2 | grep -o ',' | wc -l
